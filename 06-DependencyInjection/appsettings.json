{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "TenantNotificationPreferences": {
        "tenant-a": "Email",
        "tenant-b": "SMS",
        "tenant-c": "Push"
    },
    "AuditLoggerSettings": {
        "logLevel": "Detailed",
        "IncludeTimestamp": true,
        "IncludeTenantId": true,
        "MaxLogEntries": 1000
    }
}
